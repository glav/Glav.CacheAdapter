      <!--<setting name="CacheSpecificData" serializeAs="String">
        <value>MinPoolSize=10;MaxPoolSize=30;ConnectionTimeout=00:00:05;DeadNodeTimeout=00:00:30</value>
      </setting>-->
      <setting name="IsCacheEnabled" serializeAs="String">
        <value>True</value>
      </setting>
      <setting name="IsCacheDependencyManagementEnabled" serializeAs="String">
        <value>False</value>
      </setting>
      <!--Note: This setting a placeholder for future use. Defaults to 'Default'-->
      <setting name="DependencyManagerToUse" serializeAs="String">
        <value>Default</value>
      </setting>

    </Glav.CacheAdapter.MainConfig>
  </applicationSettings>
  <runtime>
