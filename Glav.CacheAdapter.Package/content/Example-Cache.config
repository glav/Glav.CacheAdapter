<?xml version="1.0"?>
<configuration>
  <!--Need to include the config sections below to ensure the config settings can be read-->
  <configSections>
    <sectionGroup name="applicationSettings" type="System.Configuration.ApplicationSettingsGroup, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" >
      <section name="Glav.CacheAdapter.Bootstrap.MainConfig" type="System.Configuration.ClientSettingsSection, System, Version=4.0.0.0, Culture=neutral, PublicKeyToken=b77a5c561934e089" requirePermission="false" />
    </sectionGroup>
  </configSections>
  
  <startup>
    <supportedRuntime version="v4.0" sku=".NETFramework,Version=v4.0"/>
  </startup>
  
  
  <applicationSettings>
    <Glav.CacheAdapter.Bootstrap.MainConfig>
      
      <setting name="CacheToUse" serializeAs="String">
        <!--Set this value to either 'Memory', 'Web', or 'AppFabric'-->
        <value>Memory</value>
      </setting>
      
      <setting name="SettingDistributedCacheServers" serializeAs="String">
        <!--These values represent the servers within your AppFabric distributed cache farm if AppFabric is the configured cache mechanism-->
        <value>localhost:22223,server2:22223,server3:22223</value>
      </setting>
      
    </Glav.CacheAdapter.Bootstrap.MainConfig>
  </applicationSettings>
</configuration>
